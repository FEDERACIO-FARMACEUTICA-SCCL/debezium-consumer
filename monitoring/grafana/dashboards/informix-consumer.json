{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "title": "Log Stream",
      "type": "logs",
      "gridPos": { "h": 12, "w": 24, "x": 0, "y": 0 },
      "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
      "targets": [
        {
          "expr": "{container=\"informix-consumer\"} | json | __error__=`` | line_format \"{{.level}} [{{.tag}}] {{.msg}}\"",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": false,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "dedupStrategy": "none",
        "sortOrder": "Descending"
      }
    },
    {
      "title": "CDC Events / min",
      "type": "timeseries",
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
      "targets": [
        {
          "expr": "sum by (table) (count_over_time({container=\"informix-consumer\"} | json | tag = `CDC` [1m]))",
          "legendFormat": "{{ table }}",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "custom": {
            "fillOpacity": 20,
            "lineWidth": 2,
            "spanNulls": false
          },
          "unit": "events/min"
        },
        "overrides": []
      },
      "options": {
        "legend": { "displayMode": "table", "placement": "bottom", "calcs": ["sum", "mean"] },
        "tooltip": { "mode": "multi" }
      }
    },
    {
      "title": "Errors",
      "type": "logs",
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
      "targets": [
        {
          "expr": "{container=\"informix-consumer\"} | json | level = `50` or level = `60`",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": false,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "dedupStrategy": "none",
        "sortOrder": "Descending"
      },
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      }
    },
    {
      "title": "Store Rebuild Progress",
      "type": "stat",
      "gridPos": { "h": 6, "w": 12, "x": 0, "y": 20 },
      "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
      "targets": [
        {
          "expr": "{container=\"informix-consumer\"} | json | tag = `StoreRebuild` | unwrap eventsProcessed [$__auto]",
          "legendFormat": "Events replayed",
          "refId": "A",
          "queryType": "instant"
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"]
        },
        "colorMode": "background",
        "graphMode": "area",
        "textMode": "value_and_name"
      },
      "fieldConfig": {
        "defaults": {
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null },
              { "color": "green", "value": 1 }
            ]
          }
        },
        "overrides": []
      }
    },
    {
      "title": "Pending Buffer (retries)",
      "type": "logs",
      "gridPos": { "h": 6, "w": 12, "x": 12, "y": 20 },
      "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
      "targets": [
        {
          "expr": "{container=\"informix-consumer\"} | json | tag = `PendingBuffer`",
          "refId": "A"
        }
      ],
      "options": {
        "showTime": true,
        "showLabels": false,
        "showCommonLabels": false,
        "wrapLogMessage": true,
        "prettifyLogMessage": false,
        "enableLogDetails": true,
        "dedupStrategy": "none",
        "sortOrder": "Descending"
      }
    }
  ],
  "schemaVersion": 39,
  "tags": ["informix", "cdc", "consumer"],
  "templating": {
    "list": [
      {
        "current": {},
        "hide": 2,
        "name": "DS_LOKI",
        "type": "datasource",
        "query": "loki"
      }
    ]
  },
  "time": { "from": "now-1h", "to": "now" },
  "timepicker": {},
  "timezone": "browser",
  "title": "Informix Consumer",
  "uid": "informix-consumer",
  "version": 1
}
